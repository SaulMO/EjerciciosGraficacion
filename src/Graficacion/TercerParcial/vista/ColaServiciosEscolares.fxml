<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" spacing="15" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graficacion.TercerParcial.controlador.ControllerColaServiciosEscolares">
    <VBox alignment="CENTER" spacing="15">
        <AnchorPane>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#a5b9cd" height="200.0" layoutX="0.0" layoutY="45.0" strokeType="INSIDE" width="250.0" />
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#3ca664" height="40.0" layoutX="0.0" layoutY="0.0" strokeType="INSIDE" width="250.0" />
            <Label layoutX="20.0" layoutY="12.0" text="SERVICIOS ESCOLARES">
                <font>
                    <Font name="Arial Italic" size="18.0" />
                </font></Label>
        </AnchorPane>
        <HBox alignment="center" spacing="10">
            <Button fx:id="btnLlega" text="Llega Alumno" />
            <Button fx:id="btnAtender" text="Atiende Alumno" />
        </HBox>
    </VBox>
    <Canvas fx:id="lienzo" height="300" width="600" />
</HBox>
